  "scripts": {
    "start": "ts-node-dev -r dotenv/config --respawn src/startServer.ts | pino-pretty -t",
    "test": "jest --detectOpenHandles --forceExit --passWithNoTests",
    "clean": "rimraf ./dist",
    "compile": "npm run clean && tsc",
    "cp:env": "cp ./default.env ./.env",
    "prod:start": "node ./dist/startServer.js",
    "db:sync": "node ./dist/initial/syncSchema.js",
    "dev:db:sync": "ts-node -r dotenv/config ./src/initial/syncSchema.ts",
    "db:seed": " node ./dist/initial/seedDataCreator.js",
    "dev:db:seed": " ts-node -r dotenv/config ./src/initial/seedDataCreator.ts",
    "db:resetStore": " node ./dist/initial/resetStore.js",
    "dev:db:resetStore": " ts-node -r dotenv/config ./src/initial/resetStore.ts",
    "db:replayStore": " node ./dist/initial/replayStore.js",
    "dev:db:replayStore": " ts-node -r dotenv/config ./src/initial/replayStore.ts",
    "ci:install": "npm ci",
    "lock:regen": "npm i --package-lock-only"
  },
